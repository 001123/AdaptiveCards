{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.service": "adaptive-cards",
      "uhfHeaderId": "MSDocsHeader-AdaptiveCards",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "titleSuffix": "Adaptive Cards",
      "searchScope": ["Adaptive Cards"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/AdaptiveCards"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "AdaptiveCards",
    "markdownEngineName": "markdig"
  }
}
